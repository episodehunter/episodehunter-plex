{
  "name": "episodehunter-plex",
  "version": "1.0.0",
  "description": "Plex service for episodehunter",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "start": "tsc && cp app-bridge.js logo.png index.html dist && electron ."
  },
  "keywords": [
    "plex",
    "episodehunter"
  ],
  "author": "Oskar Karlsson <kontakta@oskarkarlsson.nu>",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.6.4",
    "electron-config": "^0.2.1",
    "electron-log": "^2.0.0",
    "jsonwebtoken": "^7.3.0",
    "menubar": "^5.2.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rxjs": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.11",
    "@types/react": "^15.0.20",
    "@types/react-dom": "0.14.20",
    "electron-builder": "^16.6.1",
    "electron-packager": "^8.6.0",
    "jest": "^19.0.2",
    "marble-test": "^2.2.0",
    "react-test-renderer": "^15.4.2",
    "simple-spy": "^2.1.0",
    "ts-jest": "^19.0.5",
    "tslint": "^4.5.1",
    "tslint-react": "^2.5.0",
    "typescript": "^2.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/.*\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/vendor.js"
    ],
    "testEnvironment": "node"
  },
  "build": {
    "appId": "tv.episodehunter.plex"
  }
}
