  "author": "Oskar Karlsson <kontakta@oskarkarlsson.nu>",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.4.13",
    "electron-config": "^0.2.1",
    "electron-log": "^2.0.0",
    "electron-packager": "^8.5.0",
    "electron-reload": "^1.1.0",
    "jsonwebtoken": "^7.3.0",
    "menubar": "^5.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rxjs": "^5.0.3"
  },
  "devDependencies": {
    "@types/jest": "^18.1.1",
    "@types/node": "^6.0.58",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.20",
    "jest": "^19.0.2",
    "marble-test": "^2.1.2",
    "simple-spy": "^2.1.0",
    "ts-jest": "^19.0.0",
